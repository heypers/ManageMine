{
  "schemaVersion": 1,
  "id": "mngmine",
  "version": "${version}",
  "name": "ManageMine",
  "description": "Effortless Server Management for Minecraft",
  "authors": ["mr_fortuna"],
  "contact": {"email":  "heypers.team@gmail.com"},
  "license": "/LICENSE.txt",
  "icon": "assets/heypers/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "org.heypers.mngmine.Mngmine"
    ]
  },
  "mixins": [
    "heypers.mixins.json",
    {
      "config": "heypers.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
